{
  "name": "scrapbox-list-marker",
  "description": "Customize the scrapbox list marker.",
  "version": "0.0.1",
  "manifest_version": 3,

  "action": {
    "default_title": "scrapbox-list-marker",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://scrapbox.io/*/*"],
      "js": ["test.js"]
    }
  ],
  "permissions": ["activeTab"]
}
